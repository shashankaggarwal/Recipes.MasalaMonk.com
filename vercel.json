{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      }
    ],
    "buildCommand": "vite build",
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/dist/index.html"
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false
}
