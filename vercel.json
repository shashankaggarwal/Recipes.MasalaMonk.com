{
    "version": 2,
    "builds": [
      {
        "src": "recipe.masalamonk.com/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "recipe.masalamonk.com/dist" }
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/dist/index.html"
      }
    ]
  }
 
